{
  "name": "app",
  "version": "0.2.0",
  "private": true,
  "productName": "Gearchy",
  "scripts": {
    "quasar": "quasar",
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "typecheck": "tsc --noEmit",
    "generate:icons": "icongenie generate -m spa,pwa -i ./src/assets/logo.png",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.2",
    "@quasar/extras": "^1.0.0",
    "@types/uuid": "^8.3.4",
    "@vueuse/core": "^9.2.0",
    "dexie": "^3.2.2",
    "pinia": "^2.0.11",
    "quasar": "^2.8.3",
    "uuid": "^8.3.2",
    "vue": "3.2.39",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@octokit/core": "^4.0.4",
    "@octokit/openapi-types": "^13.2.0",
    "@octokit/types": "^7.1.0",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.0.0-beta.5",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^10.10.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "goggledy": "workspace:0.2.0",
    "highlight.js": "^11.6.0",
    "highlightjs-goggles": "workspace:*",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "unocss": "^0.45.6"
  }
}