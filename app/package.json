{
  "name": "app",
  "version": "0.2.0",
  "private": true,
  "productName": "Gearchy",
  "scripts": {
    "quasar": "quasar",
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "typecheck": "tsc --noEmit",
    "generate:icons": "icongenie generate -m spa,pwa -i ./src/assets/logo.png",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.2",
    "@quasar/extras": "^1.15.5",
    "@types/uuid": "^8.3.4",
    "@vueuse/core": "^9.5.0",
    "dexie": "^3.2.2",
    "pinia": "^2.0.24",
    "quasar": "^2.10.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@devidw/components": "^0.1.1",
    "@octokit/core": "^4.1.0",
    "@octokit/openapi-types": "^13.13.1",
    "@octokit/types": "^7.5.1",
    "@quasar/app-vite": "^1.1.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.0.0-beta.7",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^10.11.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.7.0",
    "goggledy": "workspace:0.2.0",
    "highlight.js": "^11.6.0",
    "highlightjs-goggles": "workspace:0.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "unocss": "^0.45.30",
    "vue-iso": "latest"
  }
}